{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/dist/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "5000",
    "ALLOWED_ORIGINS": "https://coursehub-frontend.vercel.app,http://localhost:3000",
    "FRONTEND_URL": "https://coursehub-frontend.vercel.app",
    "JWT_EXPIRE": "30d"
  }
} 